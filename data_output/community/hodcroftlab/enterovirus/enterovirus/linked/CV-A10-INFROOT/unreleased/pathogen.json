{
  "schemaVersion": "3.0.0",
  "attributes": {
    "name": "Coxsackievirus A10",
    "reference name": "Human coxsackievirus A10 strain Kowalik, complete genome",
    "reference accession": "AY421767.1"
  },
  "files": {
    "reference": "reference-CVA10INFROOT.fasta",
    "pathogenJson": "pathogen.json",
    "genomeAnnotation": "genome_annotation.gff3",
    "treeJson": "tree-CVA10INFROOT.json",
    "examples": "sequences-CVA10INFROOT.fasta",
    "readme": "README.md",
    "changelog": "CHANGELOG.md"
  },
  "alignmentParams": {
    "penaltyGapOpen": 15,
    "penaltyGapOpenInFrame": 20,
    "penaltyGapOpenOutOfFrame": 25,
    "kmerLength": 10,
    "kmerDistance": 50,
    "minMatchLength": 50,
    "allowedMismatches": 8,
    "minLength": 69,
    "minSeedCover": 0.35
  },
  "defaultCds": "VP1",
  "compatibility": {
    "cli": "3.0.0",
    "web": "3.0.0"
  },
  "qc": {
    "frameShifts": {
      "enabled": true,
      "scoreWeight": 100
    },
    "missingData": {
      "enabled": true,
      "missingDataThreshold": 500,
      "scoreBias": 1000
    },
    "mixedSites": {
      "enabled": true,
      "mixedSitesThreshold": 10
    },
    "privateMutations": {
      "cutoff": 100,
      "enabled": true,
      "typical": 50,
      "weightLabeledSubstitutions": 1,
      "weightReversionSubstitutions": 2,
      "weightUnlabeledSubstitutions": 1
    },
    "divergence": {
      "enabled": true,
      "maxDivergence": 0.15,
      "scoreWeight": 50
    },
    "stopCodons": {
      "enabled": true,
      "scoreWeight": 40
    }
  },
  "cdsOrderPreference": [
    "VP4",
    "VP2",
    "VP3",
    "VP1",
    "2A",
    "2B",
    "2C",
    "3A",
    "3B",
    "3C",
    "3D"
  ],
  "version": {
    "tag": "unreleased",
    "compatibility": {
      "cli": "3.0.0",
      "web": "3.0.0"
    }
  }
}
